{
  "name": "@iamcosta/url-params-hook",
  "version": "2.0.0",
  "description": "Map object props to url search parameters",
  "source": "src/index.ts",
  "exports": {
    "./*": {
      "types": "./build/types/index.d.ts",
      "require": "./build/cjs/index.js",
      "import": "./build/esm/index.js",
      "default": "./build/esm/index.js"
    }
  },
  "scripts": {
    "compile": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
    "build:clean": "rm -rf ./build",
    "build": "npm-run-all build:clean compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamcosta/url-params-hook.git"
  },
  "keywords": [
    "react",
    "URLSearchParams",
    "hook"
  ],
  "author": "Iam Barroso",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iamcosta/url-params-hook/issues"
  },
  "homepage": "https://github.com/iamcosta/url-params-hook#readme",
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "react": "^18.3.1"
  }
}
